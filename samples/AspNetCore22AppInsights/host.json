{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "maxTelemetryItemsPerSecond": 1
      }
    },
    "fileLoggingMode": "always",
    "logLevel": {
      "default": "Warning",
      "Host.Result": "Information",
      "Function": "Warning",
      "Host.Aggreator": "Trace"
    }
  }
}